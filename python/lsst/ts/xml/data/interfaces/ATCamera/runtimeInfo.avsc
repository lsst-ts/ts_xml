{
  "type" : "record",
  "name" : "runtimeInfo",
  "namespace" : "lsst.sal.ATCamera",
  "fields" : [
    {
      "name" : "timestamp",
      "type" : "long",
      "description" : "UTC timestamp",
      "units" : "millisecond"
    },
    {
      "name" : "nThreads",
      "type" : [
        "null",
        "int"
      ],
      "default" : null,
      "description" : "Number of Threads allocated by the CCS process",
      "units" : "unitless"
    },
    {
      "name" : "maxMemory",
      "type" : [
        "null",
        "long"
      ],
      "default" : null,
      "description" : "Maximum Memory available to the JVM",
      "units" : "bytes"
    },
    {
      "name" : "totalMemory",
      "type" : [
        "null",
        "long"
      ],
      "default" : null,
      "description" : "Total Memory allocated by JVM",
      "units" : "bytes"
    },
    {
      "name" : "freeMemory",
      "type" : [
        "null",
        "long"
      ],
      "default" : null,
      "description" : "Free Memory available that could be used by the JVM",
      "units" : "bytes"
    },
    {
      "name" : "processCpuLoad",
      "type" : [
        "null",
        "double"
