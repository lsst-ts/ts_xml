    {
      "name" : "systemCpuLoad",
      "type" : [
        "null",
        "double"
      ],
      "default" : null,
      "description" : "Recent cpu usage for the JVM process. This value is a double in the [0.0,1.0] interval.",
      "units" : "unitless"
