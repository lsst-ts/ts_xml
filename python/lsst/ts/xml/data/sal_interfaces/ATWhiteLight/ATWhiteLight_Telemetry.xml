<?xml version="1.0" encoding="UTF-8"?>
<?xml-stylesheet type="text/xsl" href="http://lsst-sal.tuc.noao.edu/schema/SALTelemetrySet.xsl"?>
<SALTelemetrySet xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="http://lsst-sal.tuc.noao.edu/schema/SALTelemetrySet.xsd">
  <SALTelemetry>
    <Subsystem>ATWhiteLight</Subsystem>
    <EFDB_Topic>ATWhiteLight_chillerFanSpeeds</EFDB_Topic>
    <Description>Chiller fan speeds, in revolutions per second</Description>
    <item>
      <EFDB_Name>fan1</EFDB_Name>
      <Description>fan 1 speed</Description>
      <IDL_Type>float</IDL_Type>
      <Units>Hz</Units>
      <Count>1</Count>
    </item>
    <item>
      <EFDB_Name>fan2</EFDB_Name>
      <Description>fan 2 speed</Description>
      <IDL_Type>float</IDL_Type>
      <Units>Hz</Units>
      <Count>1</Count>
    </item>
    <item>
      <EFDB_Name>fan3</EFDB_Name>
      <Description>fan 3 speed</Description>
      <IDL_Type>float</IDL_Type>
      <Units>Hz</Units>
      <Count>1</Count>
    </item>
    <item>
      <EFDB_Name>fan4</EFDB_Name>
      <Description>fan 4 speed</Description>
      <IDL_Type>float</IDL_Type>
      <Units>Hz</Units>
      <Count>1</Count>
    </item>
  </SALTelemetry>
  <SALTelemetry>
    <Subsystem>ATWhiteLight</Subsystem>
    <EFDB_Topic>ATWhiteLight_chillerTemperatures</EFDB_Topic>
    <Description>Chiller set temperature and measured temperatures</Description>
    <item>
      <EFDB_Name>setTemperature</EFDB_Name>
      <Description>Set temperature</Description>
      <IDL_Type>float</IDL_Type>
      <Units>deg_C</Units>
      <Count>1</Count>
    </item>
    <item>
      <EFDB_Name>supplyTemperature</EFDB_Name>
      <Description>Supply coolant temperature</Description>
      <IDL_Type>float</IDL_Type>
      <Units>deg_C</Units>
      <Count>1</Count>
    </item>
    <item>
      <EFDB_Name>returnTemperature</EFDB_Name>
      <Description>Return coolant temperature</Description>
      <IDL_Type>float</IDL_Type>
      <Units>deg_C</Units>
      <Count>1</Count>
    </item>
    <item>
      <EFDB_Name>ambientTemperature</EFDB_Name>
      <Description>Ambient temperature</Description>
      <IDL_Type>float</IDL_Type>
      <Units>deg_C</Units>
      <Count>1</Count>
    </item>
  </SALTelemetry>
  <SALTelemetry>
    <Subsystem>ATWhiteLight</Subsystem>
    <EFDB_Topic>ATWhiteLight_chillerCoolantFlow</EFDB_Topic>
    <Description>Chiller coolant flow rate</Description>
    <item>
      <EFDB_Name>flow</EFDB_Name>
      <Description>flow rate in liters/min</Description>
      <IDL_Type>float</IDL_Type>
      <Units>liter/min</Units>
      <Count>1</Count>
    </item>
  </SALTelemetry>
  <SALTelemetry>
    <Subsystem>ATWhiteLight</Subsystem>
    <EFDB_Topic>ATWhiteLight_chillerTECBankCurrents</EFDB_Topic>
    <Description>Chiller thermo-electric cooler drive current</Description>
    <item>
      <EFDB_Name>bank1</EFDB_Name>
      <Description>Bank 1 current</Description>
      <IDL_Type>float</IDL_Type>
      <Units>ampere</Units>
      <Count>1</Count>
    </item>
    <item>
      <EFDB_Name>bank2</EFDB_Name>
      <Description>Bank 2 current</Description>
      <IDL_Type>float</IDL_Type>
      <Units>ampere</Units>
      <Count>1</Count>
    </item>
  </SALTelemetry>
  <SALTelemetry>
    <Subsystem>ATWhiteLight</Subsystem>
    <EFDB_Topic>ATWhiteLight_chillerTECDrive</EFDB_Topic>
    <Description>Chiller thermo-electric cooler drive status</Description>
    <item>
      <EFDB_Name>isCooling</EFDB_Name>
      <Description>Cooling if true, else heating.</Description>
      <IDL_Type>boolean</IDL_Type>
      <Units>unitless</Units>
      <Count>1</Count>
    </item>
    <item>
      <EFDB_Name>level</EFDB_Name>
      <Description>Drive level</Description>
      <IDL_Type>float</IDL_Type>
      <Units>percent</Units>
      <Count>1</Count>
    </item>
  </SALTelemetry>
</SALTelemetrySet>
