<SALEventSet xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="http://lsst-sal.tuc.noao.edu/schema/SALEventSet.xsd">
  <Enumeration>AmebaMode_Off,AmebaMode_Auto,AmebaMode_Manual</Enumeration>
  <Enumeration>ProgramControl_Stop,ProgramControl_Start,ProgramControl_Restart,ProgramControl_NoAction</Enumeration>
  <Enumeration>SkyStatus_Clear,SkyStatus_LightCoverage,SkyStatus_Cloudy,SkyStatus_Rain,SkyStatus_Snow</Enumeration>
  <Enumeration>ScopeMotion_Park,ScopeMotion_Slew,ScopeMotion_Stand,ScopeMotion_Tracking</Enumeration>
  <Enumeration>Dome_Undefined,Dome_Opened,Dome_Closed,Dome_Stated</Enumeration>
  <Enumeration>AmebaState_Inactive,AmebaState_Waiting,AmebaState_Slewing,AmebaState_Tracking,AmebaState_Focusing,AmebaState_Monitoring</Enumeration>
  <SALEvent>
    <Subsystem>DIMM</Subsystem>
    <EFDB_Topic>DIMM_logevent_moduleStatus</EFDB_Topic>
    <Description>Module statuses</Description>
    <item>
      <EFDB_Name>masterStatus</EFDB_Name>
      <Description>Subsystem status for Master module.</Description>
      <IDL_Type>boolean</IDL_Type>
      <Units>unitless</Units>
      <Count>1</Count>
    </item>
    <item>
      <EFDB_Name>masterFailState</EFDB_Name>
      <Description>
        Failure status for the Master module. This state resembles the is-failed property of systemctl.
      </Description>
      <IDL_Type>boolean</IDL_Type>
      <Units>unitless</Units>
      <Count>1</Count>
    </item>
    <item>
      <EFDB_Name>amebaStatus</EFDB_Name>
      <Description>Subsystem status for AMEBA module.</Description>
      <IDL_Type>boolean</IDL_Type>
      <Units>unitless</Units>
      <Count>1</Count>
    </item>
    <item>
      <EFDB_Name>amebaFailState</EFDB_Name>
      <Description>
        Failure status for the AMEBA module. This state resembles the is-failed property of systemctl.
      </Description>
      <IDL_Type>boolean</IDL_Type>
      <Units>unitless</Units>
      <Count>1</Count>
    </item>
    <item>
      <EFDB_Name>dimmStatus</EFDB_Name>
      <Description>Subsystem status for DIMM processing module.</Description>
      <IDL_Type>boolean</IDL_Type>
      <Units>unitless</Units>
      <Count>1</Count>
    </item>
    <item>
      <EFDB_Name>dimmFailState</EFDB_Name>
      <Description>
        Failure status for the DIMM module. This state resembles the is-failed property of systemctl.
      </Description>
      <IDL_Type>boolean</IDL_Type>
      <Units>unitless</Units>
      <Count>1</Count>
    </item>
    <item>
      <EFDB_Name>domeStatus</EFDB_Name>
      <Description>Subsystem status for Dome module.</Description>
      <IDL_Type>boolean</IDL_Type>
      <Units>unitless</Units>
      <Count>1</Count>
    </item>
    <item>
      <EFDB_Name>domeFailState</EFDB_Name>
      <Description>
        Failure status for the Dome module. This state resembles the is-failed property of systemctl.
      </Description>
      <IDL_Type>boolean</IDL_Type>
      <Units>unitless</Units>
      <Count>1</Count>
    </item>
    <item>
      <EFDB_Name>meteorologyStatus</EFDB_Name>
      <Description>Subsystem status for Meteorology module. Note the spelling of this topic name.</Description>
      <IDL_Type>boolean</IDL_Type>
      <Units>unitless</Units>
      <Count>1</Count>
    </item>
    <item>
      <EFDB_Name>meteorologyFailState</EFDB_Name>
      <Description>
        Failure status for the Meteorology module. This state resembles the is-failed property of systemctl.
      </Description>
      <IDL_Type>boolean</IDL_Type>
      <Units>unitless</Units>
      <Count>1</Count>
    </item>
    <item>
      <EFDB_Name>scopeStatus</EFDB_Name>
      <Description>Subsystem status for Scope module.</Description>
      <IDL_Type>boolean</IDL_Type>
      <Units>unitless</Units>
      <Count>1</Count>
    </item>
    <item>
      <EFDB_Name>scopeFailState</EFDB_Name>
      <Description>
        Failure status for the Scope module. This state resembles the is-failed property of systemctl.
      </Description>
      <IDL_Type>boolean</IDL_Type>
      <Units>unitless</Units>
      <Count>1</Count>
    </item>
    <item>
      <EFDB_Name>dimmKornilovErrors</EFDB_Name>
      <Description>
        Error messages, if any, for the dimm-tool Kornilov server, or an empty string if there is no error for
        that server.
      </Description>
      <IDL_Type>string</IDL_Type>
      <Units>unitless</Units>
      <Count>1</Count>
    </item>
    <item>
      <EFDB_Name>amebaKornilovErrors</EFDB_Name>
      <Description>
        Error messages, if any, for the ameba Kornilov server, or an empty string if there is no error for
        that server.
      </Description>
      <IDL_Type>string</IDL_Type>
      <Units>unitless</Units>
      <Count>1</Count>
    </item>
    <item>
      <EFDB_Name>meteoKornilovErrors</EFDB_Name>
      <Description>
        Error messages, if any, for the tt-meteo Kornilov server, or an empty string if there is no error for
        that server.
      </Description>
      <IDL_Type>string</IDL_Type>
      <Units>unitless</Units>
      <Count>1</Count>
    </item>
    <item>
      <EFDB_Name>domeKornilovErrors</EFDB_Name>
      <Description>
        Error messages, if any, for the tt-dome-astrohaven Kornilov server, or an empty string if there is no error
        for that server.
      </Description>
      <IDL_Type>string</IDL_Type>
      <Units>unitless</Units>
      <Count>1</Count>
    </item>
    <item>
      <EFDB_Name>scopeKornilovErrors</EFDB_Name>
      <Description>
        Error messages, if any, for the tt-scope Kornilov server, or an empty string if there is no error
        for that server.
      </Description>
      <IDL_Type>string</IDL_Type>
      <Units>unitless</Units>
      <Count>1</Count>
    </item>
    <item>
      <EFDB_Name>scopeErrors</EFDB_Name>
      <Description><![CDATA[
          Telescope status and error reset reported by DIMM. This is a comma separated list of all
          saved error events in the format <error>:<status>:<device>[:<comment>][,...].
          ]]></Description>
      <IDL_Type>string</IDL_Type>
      <Units>unitless</Units>
      <Count>1</Count>
    </item>
  </SALEvent>
  <SALEvent>
    <Subsystem>DIMM</Subsystem>
    <EFDB_Topic>DIMM_logevent_dimmMeasurement</EFDB_Topic>
    <Description>A DIMM measurement</Description>
    <item>
      <EFDB_Name>timestamp</EFDB_Name>
      <Description>The time of the event. (DIMM.TIMESTAMP)</Description>
      <IDL_Type>double</IDL_Type>
      <Units>second</Units>
      <Count>1</Count>
    </item>
    <item>
      <EFDB_Name>expiresAt</EFDB_Name>
      <Description>Timestamp (UTC) until when the measurement is deemed valid.</Description>
      <IDL_Type>double</IDL_Type>
      <Units>second</Units>
      <Count>1</Count>
    </item>
    <item>
      <EFDB_Name>expiresIn</EFDB_Name>
      <Description>Duration the measurement is deemed valid since publication.</Description>
      <IDL_Type>float</IDL_Type>
      <Units>second</Units>
      <Count>1</Count>
    </item>
    <item>
      <EFDB_Name>secz</EFDB_Name>
      <Description>Airmass of the observation (DIMM.AIRMASS)</Description>
      <IDL_Type>float</IDL_Type>
      <Units>am</Units>
      <Count>1</Count>
    </item>
    <item>
      <EFDB_Name>fwhm</EFDB_Name>
      <Description>Combined full width half maximum. (DIMM.SEEING)</Description>
      <IDL_Type>float</IDL_Type>
      <Units>arcsec</Units>
      <Count>1</Count>
    </item>
    <item>
      <EFDB_Name>fwhmLowFrequency</EFDB_Name>
      <Description>Low-frequency part of the last seeing estimation. (DIMM.SEEING_LOWFREQ)</Description>
      <IDL_Type>float</IDL_Type>
      <Units>arcsec</Units>
      <Count>1</Count>
    </item>
    <item>
      <EFDB_Name>flux</EFDB_Name>
      <Description>Total average flux this time bin</Description>
      <IDL_Type>float</IDL_Type>
      <Units>adu</Units>
      <Count>1</Count>
    </item>
    <item>
      <EFDB_Name>fluxL</EFDB_Name>
      <Description>Flux (left spot) (DIMM.FLUX_LEFT)</Description>
      <IDL_Type>float</IDL_Type>
      <Units>adu</Units>
      <Count>1</Count>
    </item>
    <item>
      <EFDB_Name>strehlL</EFDB_Name>
      <Description>Strehl-ratio (left spot) (DIMM.STREHL_LEFT)</Description>
      <IDL_Type>float</IDL_Type>
      <Units>unitless</Units>
      <Count>1</Count>
    </item>
    <item>
      <EFDB_Name>fluxR</EFDB_Name>
      <Description>Flux (right spot) (DIMM.FLUX_RIGHT)</Description>
      <IDL_Type>float</IDL_Type>
      <Units>adu</Units>
      <Count>1</Count>
    </item>
    <item>
      <EFDB_Name>rootMeanSquareFluxLeft</EFDB_Name>
      <Description>Integral flux root mean square for the left aperture image (DIMM.FLUX_RMS_LEFT)</Description>
      <IDL_Type>float</IDL_Type>
      <Units>adu</Units>
      <Count>1</Count>
    </item>
    <item>
      <EFDB_Name>rootMeanSquareFluxRight</EFDB_Name>
      <Description>Integral flux root mean square for the right aperture image (DIMM.FLUX_RMS_RIGHT)</Description>
      <IDL_Type>float</IDL_Type>
      <Units>adu</Units>
      <Count>1</Count>
    </item>
    <item>
      <EFDB_Name>strehlR</EFDB_Name>
      <Description>Strehl-ratio (right spot) (DIMM.STREHL_RIGHT)</Description>
      <IDL_Type>float</IDL_Type>
      <Units>unitless</Units>
      <Count>1</Count>
    </item>
  </SALEvent>
  <SALEvent>
    <Subsystem>DIMM</Subsystem>
    <EFDB_Topic>DIMM_logevent_manualTarget</EFDB_Topic>
    <Description>The most recently specified manual pointing target.</Description>
    <item>
      <EFDB_Name>targetName</EFDB_Name>
      <Description>Manual target name.</Description>
      <IDL_Type>string</IDL_Type>
      <Units>unitless</Units>
      <Count>1</Count>
    </item>
    <item>
      <EFDB_Name>rightAscension</EFDB_Name>
      <Description>Right ascension (J2000.0).</Description>
      <IDL_Type>float</IDL_Type>
      <Units>deg</Units>
      <Count>1</Count>
    </item>
    <item>
      <EFDB_Name>declination</EFDB_Name>
      <Description>Declination (J2000.0).</Description>
      <IDL_Type>float</IDL_Type>
      <Units>deg</Units>
      <Count>1</Count>
    </item>
    <item>
      <EFDB_Name>brightness</EFDB_Name>
      <Description>Apparent visual brightness in the V-band.</Description>
      <IDL_Type>float</IDL_Type>
      <Units>mag</Units>
      <Count>1</Count>
    </item>
    <item>
      <EFDB_Name>color</EFDB_Name>
      <Description>Object color in difference between the B- and V-band.</Description>
      <IDL_Type>float</IDL_Type>
      <Units>mag</Units>
      <Count>1</Count>
    </item>
    <item>
      <EFDB_Name>stellarClass</EFDB_Name>
      <Description>Yerkes spectral classification of the object, e.g., 'G5III'.</Description>
      <IDL_Type>string</IDL_Type>
      <Units>unitless</Units>
      <Count>1</Count>
    </item>
    <item>
      <EFDB_Name>stellarClassFile</EFDB_Name>
      <Description>Selected spectral file on the DIMM filesystem.</Description>
      <IDL_Type>string</IDL_Type>
      <Units>unitless</Units>
      <Count>1</Count>
    </item>
  </SALEvent>
  <SALEvent>
    <Subsystem>DIMM</Subsystem>
    <EFDB_Topic>DIMM_logevent_currentTarget</EFDB_Topic>
    <Description>The current target under observation.</Description>
    <item>
      <EFDB_Name>targetName</EFDB_Name>
      <Description>Current target name.</Description>
      <IDL_Type>string</IDL_Type>
      <Units>unitless</Units>
      <Count>1</Count>
    </item>
    <item>
      <EFDB_Name>rightAscension</EFDB_Name>
      <Description>Right ascension (J2000.0).</Description>
      <IDL_Type>float</IDL_Type>
      <Units>deg</Units>
      <Count>1</Count>
    </item>
    <item>
      <EFDB_Name>declination</EFDB_Name>
      <Description>Declination (J2000.0).</Description>
      <IDL_Type>float</IDL_Type>
      <Units>deg</Units>
      <Count>1</Count>
    </item>
    <item>
      <EFDB_Name>brightness</EFDB_Name>
      <Description>Apparent visual brightness in the V-band.</Description>
      <IDL_Type>float</IDL_Type>
      <Units>mag</Units>
      <Count>1</Count>
    </item>
    <item>
      <EFDB_Name>color</EFDB_Name>
      <Description>Object color in difference between the B- and V-band.</Description>
      <IDL_Type>float</IDL_Type>
      <Units>mag</Units>
      <Count>1</Count>
    </item>
    <item>
      <EFDB_Name>stellarClassFile</EFDB_Name>
      <Description>Current target selected spectral file on the DIMM filesystem.</Description>
      <IDL_Type>string</IDL_Type>
      <Units>unitless</Units>
      <Count>1</Count>
    </item>
    <item>
      <EFDB_Name>startTime</EFDB_Name>
      <Description>Time when the scope was requested to track the object.</Description>
      <IDL_Type>float</IDL_Type>
      <Units>second</Units>
      <Count>1</Count>
    </item>
  </SALEvent>
</SALEventSet>
