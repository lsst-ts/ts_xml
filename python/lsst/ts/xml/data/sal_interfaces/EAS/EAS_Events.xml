<?xml version="1.0" encoding="UTF-8"?>
<?xml-stylesheet type="text/xsl" href="http://lsst-sal.tuc.noao.edu/schema/SALEventSet.xsl"?>
<SALEventSet xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="http://lsst-sal.tuc.noao.edu/schema/SALEventSet.xsd">
  <SALEvent>
    <Subsystem>EAS</Subsystem>
    <EFDB_Topic>EAS_logevent_internalCommand</EFDB_Topic>
    <Description>Report the error code resulting from an internal command.</Description>
    <item>
      <EFDB_Name>commandObject</EFDB_Name>
      <Description>The error code being published.</Description>
      <IDL_Type>byte</IDL_Type>
      <Units>unitless</Units>
      <Count>800</Count>
    </item>
  </SALEvent>
  <SALEvent>
    <Subsystem>EAS</Subsystem>
    <EFDB_Topic>EAS_logevent_loopTimeOutOfRange</EFDB_Topic>
    <Description>Report whether the loop time is out of range or not.</Description>
    <item>
      <EFDB_Name>loopTimeOutOfRange</EFDB_Name>
      <Description>Loop time out of range.</Description>
      <IDL_Type>boolean</IDL_Type>
      <Units>unitless</Units>
      <Count>1</Count>
    </item>
  </SALEvent>
  <SALEvent>
    <Subsystem>EAS</Subsystem>
    <EFDB_Topic>EAS_logevent_rejectedCommand</EFDB_Topic>
    <Description>Report the a rejected command.</Description>
    <item>
      <EFDB_Name>command</EFDB_Name>
      <Description>Name of the rejected command.</Description>
      <IDL_Type>string</IDL_Type>
      <IDL_Size>1</IDL_Size>
      <Units>unitless</Units>
      <Count>1</Count>
    </item>
    <item>
      <EFDB_Name>state</EFDB_Name>
      <Description>Current CSC state.</Description>
      <IDL_Type>string</IDL_Type>
      <IDL_Size>1</IDL_Size>
      <Units>unitless</Units>
      <Count>1</Count>
    </item>
    <item>
      <EFDB_Name>timestamp</EFDB_Name>
      <Description>Timestamp when the command was rejected.</Description>
      <IDL_Type>double</IDL_Type>
      <Units>unitless</Units>
      <Count>1</Count>
    </item>
  </SALEvent>
</SALEventSet>
