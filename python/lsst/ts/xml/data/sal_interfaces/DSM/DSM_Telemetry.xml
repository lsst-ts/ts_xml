<?xml version="1.0" encoding="UTF-8"?>
<?xml-stylesheet type="text/xsl" href="http://lsst-sal.tuc.noao.edu/schema/SALTelemetrySet.xsl"?>
<SALTelemetrySet xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="http://lsst-sal.tuc.noao.edu/schema/SALTelemetrySet.xsd">
  <SALTelemetry>
    <Subsystem>DSM</Subsystem>
    <EFDB_Topic>DSM_domeSeeing</EFDB_Topic>
    <Description>Measured quantities associated with dome seeing from a DSM device.</Description>
    <item>
      <EFDB_Name>dsmIndex</EFDB_Name>
      <Description>The index of the DSM unit.</Description>
      <IDL_Type>long</IDL_Type>
      <Units>unitless</Units>
      <Count>1</Count>
    </item>
    <item>
      <EFDB_Name>timestampCurrent</EFDB_Name>
      <Description>The TAI timestamp of the current buffer measurement.</Description>
      <IDL_Type>double</IDL_Type>
      <Units>second</Units>
      <Count>1</Count>
    </item>
    <item>
      <EFDB_Name>timestampFirstMeasurement</EFDB_Name>
      <Description>The TAI timestamp of the first measurement in the buffer.</Description>
      <IDL_Type>double</IDL_Type>
      <Units>second</Units>
      <Count>1</Count>
    </item>
    <item>
      <EFDB_Name>timestampLastMeasurement</EFDB_Name>
      <Description>The TAI timestamp of the last measurement in the buffer.</Description>
      <IDL_Type>double</IDL_Type>
      <Units>second</Units>
      <Count>1</Count>
    </item>
    <item>
      <EFDB_Name>rmsX</EFDB_Name>
      <Description>The standard deviation of the buffer centroids in the X direction.</Description>
      <IDL_Type>double</IDL_Type>
      <Units>arcsec</Units>
      <Count>1</Count>
    </item>
    <item>
      <EFDB_Name>rmsY</EFDB_Name>
      <Description>The standard deviation of the buffer centroids in the Y direction.</Description>
      <IDL_Type>double</IDL_Type>
      <Units>arcsec</Units>
      <Count>1</Count>
    </item>
    <item>
      <EFDB_Name>centroidX</EFDB_Name>
      <Description>The average spot position of the buffer centroids in the X direction.</Description>
      <IDL_Type>double</IDL_Type>
      <Units>pixel</Units>
      <Count>1</Count>
    </item>
    <item>
      <EFDB_Name>centroidY</EFDB_Name>
      <Description>The average spot position of the buffer centroids in the Y direction.</Description>
      <IDL_Type>double</IDL_Type>
      <Units>pixel</Units>
      <Count>1</Count>
    </item>
    <item>
      <EFDB_Name>flux</EFDB_Name>
      <Description>The average of the frame fluxes (ADU) in the buffer tracking the centroids.</Description>
      <IDL_Type>double</IDL_Type>
      <Units>unitless</Units>
      <Count>1</Count>
    </item>
    <item>
      <EFDB_Name>maxADC</EFDB_Name>
      <Description>The average of the frame maximum ADC values in the buffer tracking the centroids.</Description>
      <IDL_Type>double</IDL_Type>
      <Units>unitless</Units>
      <Count>1</Count>
    </item>
    <item>
      <EFDB_Name>fwhm</EFDB_Name>
      <Description>The average of the spot full-width, half-maxes in the buffer tracking the centroids.</Description>
      <IDL_Type>double</IDL_Type>
      <Units>pixel</Units>
      <Count>1</Count>
    </item>
  </SALTelemetry>
</SALTelemetrySet>
