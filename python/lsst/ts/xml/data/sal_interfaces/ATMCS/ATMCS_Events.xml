      <EFDB_Name>detailedState</EFDB_Name>
      <Description>The substate or extra states of the CSC </Description>
      <!-- Enumeration: DetailedState -->
      <IDL_Type>long</IDL_Type>
      <Units>unitless</Units>
      <Count>1</Count>
    </item>
  </SALEvent>
  <!-- High-level events -->
