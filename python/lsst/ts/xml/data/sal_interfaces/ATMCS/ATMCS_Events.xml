  <!-- Standard events -->
  <SALEvent>
    <Subsystem>ATMCS</Subsystem>
    <EFDB_Topic>ATMCS_logevent_detailedState</EFDB_Topic>
    <Description>The substate or extra states of the CSC.</Description>
