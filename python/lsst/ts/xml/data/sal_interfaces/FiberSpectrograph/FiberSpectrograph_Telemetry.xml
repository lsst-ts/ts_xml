<?xml version="1.0" encoding="UTF-8"?>
<?xml-stylesheet type="text/xsl" href="http://lsst-sal.tuc.noao.edu/schema/SALTelemetrySet.xsl"?>
<SALTelemetrySet xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="http://lsst-sal.tuc.noao.edu/schema/SALTelemetrySet.xsd">
  <SALTelemetry>
    <Subsystem>FiberSpectrograph</Subsystem>
    <EFDB_Topic>FiberSpectrograph_temperature</EFDB_Topic>
    <Description>Temperature of the FiberSpectrograph</Description>
    <item>
      <EFDB_Name>temperature</EFDB_Name>
      <Description>Measured internal spectrograph temperature.</Description>
      <IDL_Type>double</IDL_Type>
      <Units>deg_C</Units>
      <Count>1</Count>
    </item>
    <item>
      <EFDB_Name>setpoint</EFDB_Name>
      <Description>Spectrograph temperature setpoint.</Description>
      <IDL_Type>double</IDL_Type>
      <Units>deg_C</Units>
      <Count>1</Count>
    </item>
  </SALTelemetry>
</SALTelemetrySet>
