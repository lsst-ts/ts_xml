        <EFDB_Topic>ATMCS_mountEncoders</EFDB_Topic>
        <item>
            <EFDB_Name>elevationCalculatedAngle</EFDB_Name>
            <Description>Elevation axis position computed from the axis encoders.</Description>
            <IDL_Type>double</IDL_Type>
            <Units>deg</Units>
            <Count>1</Count>
        </item>
        <item>
            <EFDB_Name>azimuthCalculatedAngle</EFDB_Name>
            <Description>Azimuth axis position computed from the axis encoders. The range is the hard stop limits (L3), approximately -280 to 280.</Description>
            <IDL_Type>double</IDL_Type>
            <Units>deg</Units>
            <Count>1</Count>
        </item>
        <item>
            <EFDB_Name>nasmyth1CalculatedAngle</EFDB_Name>
            <Description>Nasmyth 1 axis position computed from the axis encoders, or nan if this axis is not in use. The range is the software limits (L1), approximately -175 to 175. Increasing the range requires installation of an additional switch. </Description>
            <IDL_Type>double</IDL_Type>
            <Units>deg</Units>
            <Count>1</Count>
        </item>
        <item>
            <EFDB_Name>nasmyth2CalculatedAngle</EFDB_Name>
            <Description>Nasmyth 2 axis position computed from the axis encoders, or nan if this axis is not in use. The range is the hard stop limits (L3), approximately -175 to 175. Increasing the range requires installation of an additional switch.</Description>
            <IDL_Type>double</IDL_Type>
            <Units>deg</Units>
            <Count>1</Count>
        </item>
