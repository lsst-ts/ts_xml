    <SALTelemetry>
        <Subsystem>ATMCS</Subsystem>
        <Version>3.8.41</Version>
        <Author/>
	<EFDB_Topic>ATMCS_trajectory</EFDB_Topic>
	<Description>Telescope trajectory Position/Velocity/Time from the ATMCS trajectory generator at 100Hz, starting at the specified time.</Description>
        <item>
            <EFDB_Name>cRIO_timestamp</EFDB_Name>
            <Description>Time for the first sample of each telemetry item in this topic, as measured by the cRIO clock, as unix time.</Description>
            <IDL_Type>double</IDL_Type>
            <Units>s</Units>
            <Count>1</Count>
        </item>
	<item>
            <EFDB_Name>elevation</EFDB_Name>
            <Description>Target mount elevation angle at 100Hz beginning at the specified time.</Description>
            <IDL_Type>double</IDL_Type>
            <Units>deg</Units>
            <Count>100</Count>
        </item>
        <item>
            <EFDB_Name>elevationVelocity</EFDB_Name>
            <Description>Target mount elevation angle velocity at 100Hz beginning at the specified time.</Description>
            <IDL_Type>double</IDL_Type>
            <Units>deg / s</Units>
            <Count>100</Count>
        </item>
        <item>
            <EFDB_Name>azimuth</EFDB_Name>
            <Description>Target mount azimuth angle at 100Hz beginning at the specified time. The allowed range is 0 to 360.</Description>
            <IDL_Type>double</IDL_Type>
            <Units>deg</Units>
            <Count>100</Count>
        </item>
        <item>
            <EFDB_Name>azimuthVelocity</EFDB_Name>
            <Description>Target mount azimuth velocity at 100Hz beginning at the specified time.</Description>
            <IDL_Type>double</IDL_Type>
            <Units>deg / s</Units>
            <Count>100</Count>
        </item>
        <item>
            <EFDB_Name>nasmyth1RotatorAngle</EFDB_Name>
            <Description>Target Nasmyth 1 rotator angle at 100Hz beginning at the specified time.</Description>
            <IDL_Type>double</IDL_Type>
            <Units>deg</Units>
            <Count>100</Count>
        </item>
        <item>
            <EFDB_Name>nasmyth1RotatorAngleVelocity</EFDB_Name>
            <Description>Target Nasmyth 1 rotator angle velocity at 100Hz beginning at the specified time.</Description>
            <IDL_Type>double</IDL_Type>
            <Units>deg / s</Units>
            <Count>100</Count>
        </item>
        <item>
            <EFDB_Name>nasmyth2RotatorAngle</EFDB_Name>
            <Description>Target Nasmyth 1 rotator angle at 100Hz beginning at the specified time.</Description>
            <IDL_Type>double</IDL_Type>
            <Units>deg</Units>
            <Count>100</Count>
        </item>
        <item>
            <EFDB_Name>nasmyth2RotatorAngleVelocity</EFDB_Name>
            <Description>Target Nasmyth 1 rotator angle velocity at 100Hz beginning at the specified time.</Description>
            <IDL_Type>double</IDL_Type>
            <Units>deg / s</Units>
            <Count>100</Count>
        </item>
    </SALTelemetry>
