<?xml version="1.0" encoding="UTF-8"?>
<?xml-stylesheet type="text/xsl" href="http://lsst-sal.tuc.noao.edu/schema/SALCommandSet.xsl"?>
<SALCommandSet xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="http://lsst-sal.tuc.noao.edu/schema/SALCommandSet.xsd">
    <SALCommand>
        <Subsystem>ATMCS</Subsystem>
        <Version>3.8.41</Version>
        <Author/>
        <EFDB_Topic>ATMCS_command_startTracking</EFDB_Topic>
        <Alias>startTracking</Alias>
        <Device/>
        <Property/>
        <Action/>
        <Value/>
        <Description>Enable tracking. Once you issue this command you must call trackTarget at regular intervals until you are done tracking, at which point call stopTracking.</Description>
        <Explanation>http://sal.lsst.org</Explanation>
        <item>
            <EFDB_Name>ignored</EFDB_Name>
            <Description/>
            <IDL_Type>boolean</IDL_Type>
            <Units/>
            <Count>1</Count>
        </item>
    </SALCommand>
    <SALCommand>
        <Subsystem>ATMCS</Subsystem>
        <Version>3.8.41</Version>
        <Author/>
        <EFDB_Topic>ATMCS_command_trackTarget</EFDB_Topic>
        <Alias>trackTarget</Alias>
        <Device/>
        <Property/>
        <Action/>
        <Value/>
        <Description>Specify position, velocity and time for all axes except M3. This command returns immediately.</Description>
        <Explanation>http://sal.lsst.org</Explanation>
        <item>
            <EFDB_Name>azimuth</EFDB_Name>
            <Description>Mount azimuth angle at the specified time. The allowed range is 0 to 360.</Description>
            <IDL_Type>double</IDL_Type>
            <Units>deg</Units>
            <Count>1</Count>
        </item>
        <item>
            <EFDB_Name>azimuthDirection</EFDB_Name>
            <Description>Mount azimuth wrap.</Description>
            <!-- Enumeration: AzimuthDirection -->
            <IDL_Type>long</IDL_Type>
            <Units/>
            <Count>1</Count>
        </item>
        <item>
            <EFDB_Name>azimuthVelocity</EFDB_Name>
            <Description>Mount azimuth velocity at the specified time.</Description>
            <IDL_Type>double</IDL_Type>
            <Units>deg/sec</Units>
            <Count>1</Count>
        </item>
        <item>
            <EFDB_Name>elevation</EFDB_Name>
            <Description>Mount elevation angle at the specified time.</Description>
            <IDL_Type>double</IDL_Type>
            <Units>deg</Units>
            <Count>1</Count>
        </item>
        <item>
            <EFDB_Name>elevationVelocity</EFDB_Name>
            <Description>Mount elevation angle velocity at the specified time.</Description>
            <IDL_Type>double</IDL_Type>
            <Units>deg/sec</Units>
            <Count>1</Count>
        </item>
        <item>
            <EFDB_Name>nasmyth1RotatorAngle</EFDB_Name>
            <Description>Nasmyth 1 rotator angle at the specified time.</Description>
            <IDL_Type>double</IDL_Type>
            <Units>deg</Units>
            <Count>1</Count>
        </item>
        <item>
            <EFDB_Name>nasmyth1RotatorAngleVelocity</EFDB_Name>
            <Description>Nasmyth 1 rotator angle velocity at the specified time.</Description>
            <IDL_Type>double</IDL_Type>
            <Units>deg/sec</Units>
            <Count>1</Count>
        </item>
        <item>
            <EFDB_Name>nasmyth2RotatorAngle</EFDB_Name>
            <Description>Nasmyth 1 rotator angle at the specified time.</Description>
            <IDL_Type>double</IDL_Type>
            <Units>deg</Units>
            <Count>1</Count>
        </item>
        <item>
            <EFDB_Name>nasmyth2RotatorAngleVelocity</EFDB_Name>
            <Description>Nasmyth 1 rotator angle velocity at the specified time.</Description>
            <IDL_Type>double</IDL_Type>
            <Units>deg/sec</Units>
            <Count>1</Count>
        </item>
        <item>
            <EFDB_Name>time</EFDB_Name>
            <Description>TAI date at which the commanded positions and velocities apply, as unix time.</Description>
            <IDL_Type>double</IDL_Type>
            <Units>sec</Units>
            <Count>1</Count>
        </item>
        <item>
            <EFDB_Name>trackId</EFDB_Name>
            <Description>Target identifier.</Description>
            <IDL_Type>long</IDL_Type>
            <Units/>
            <Count>1</Count>
        </item>
    </SALCommand>
    <SALCommand>
        <Subsystem>ATMCS</Subsystem>
        <Version>3.8.41</Version>
        <Author/>
        <EFDB_Topic>ATMCS_command_setInstrumentPort</EFDB_Topic>
        <Alias>setInstrumentPort</Alias>
        <Device/>
        <Property/>
        <Action/>
        <Value/>
        <Description>Rotate M3 to a specified instrument port. This command returns before M3 has finished moving.</Description>
        <Explanation>http://sal.lsst.org</Explanation>
        <item>
            <EFDB_Name>port</EFDB_Name>
            <Description>Desired instrument port.</Description>
            <!-- Enumeration: M3ExitPort -->
            <IDL_Type>long</IDL_Type>
            <Units/>
            <Count>1</Count>
        </item>
    </SALCommand>
    <SALCommand>
        <Subsystem>ATMCS</Subsystem>
        <Version>3.8.41</Version>
        <Author/>
        <EFDB_Topic>ATMCS_command_stopTracking</EFDB_Topic>
        <Alias>stopTracking</Alias>
        <Device/>
        <Property/>
        <Action/>
        <Value/>
        <Description>Halt all axes except M3 rotation, and disable tracking. This command returns before the axes finish halting.</Description>
        <Explanation>http://sal.lsst.org</Explanation>
        <item>
            <EFDB_Name>ignored</EFDB_Name>
            <Description/>
            <IDL_Type>boolean</IDL_Type>
            <Units/>
            <Count>1</Count>
        </item>
    </SALCommand>
</SALCommandSet>
