<?xml version="1.0" encoding="UTF-8"?>
<?xml-stylesheet type="text/xsl" href="http://lsst-sal.tuc.noao.edu/schema/SALTelemetrySet.xsl"?>
<SALTelemetrySet xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="http://lsst-sal.tuc.noao.edu/schema/SALTelemetrySet.xsd">
  <SALTelemetry>
    <Subsystem>Guider</Subsystem>
    <EFDB_Topic>Guider_offsets</EFDB_Topic>
    <Description>Computed guider offsets. The values are computed in the instrument reference frame and need to be de-rotated accordingly by the pointing.</Description>
    <item>
      <EFDB_Name>x</EFDB_Name>
      <Description>Guider offset in the (instrument) x-axis direction.</Description>
      <IDL_Type>double</IDL_Type>
      <Units>arcsec</Units>
      <Count>1</Count>
    </item>
    <item>
      <EFDB_Name>y</EFDB_Name>
      <Description>Guider offset in the (instrument) y-axis direction.</Description>
      <IDL_Type>double</IDL_Type>
      <Units>arcsec</Units>
      <Count>1</Count>
    </item>
  </SALTelemetry>
</SALTelemetrySet>
