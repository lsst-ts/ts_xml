<?xml version="1.0" encoding="UTF-8"?>
<?xml-stylesheet type="text/xsl" href="http://lsst-sal.tuc.noao.edu/schema/SALTelemetrySet.xsl"?>
<SALTelemetrySet xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="http://lsst-sal.tuc.noao.edu/schema/SALTelemetrySet.xsd">
  <SALTelemetry>
    <Subsystem>ATMonochromator</Subsystem>
    <EFDB_Topic>ATMonochromator_timestamp</EFDB_Topic>
    <item>
      <EFDB_Name>timestamp</EFDB_Name>
      <Description>The system time (TAI unix seconds).</Description>
      <IDL_Type>double</IDL_Type>
      <Units>second</Units>
      <Count>1</Count>
    </item>
  </SALTelemetry>
  <SALTelemetry>
    <Subsystem>ATMonochromator</Subsystem>
    <EFDB_Topic>ATMonochromator_loopTime</EFDB_Topic>
    <item>
      <EFDB_Name>loopTime</EFDB_Name>
      <Description>The actual loop time.</Description>
      <IDL_Type>double</IDL_Type>
      <Units>ms</Units>
      <Count>1</Count>
    </item>
  </SALTelemetry>
</SALTelemetrySet>
