<?xml version="1.0" encoding="UTF-8"?>
<?xml-stylesheet type="text/xsl" href="http://lsst-sal.tuc.noao.edu/schema/SALTelemetrySet.xsl"?>
<SALTelemetrySet xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="http://lsst-sal.tuc.noao.edu/schema/SALTelemetrySet.xsd">
  <SALTelemetry>
    <Subsystem>Scheduler</Subsystem>
    <EFDB_Topic>Scheduler_observatoryState</EFDB_Topic>
    <Description>Information about the current state of the observatory, as seen by the Scheduler.</Description>
    <item>
      <EFDB_Name>timestamp</EFDB_Name>
      <Description>Current timestamp at the observatory</Description>
      <IDL_Type>double</IDL_Type>
      <Units>second</Units>
      <Count>1</Count>
    </item>
    <item>
      <EFDB_Name>ra</EFDB_Name>
      <Description>Current pointing RA.</Description>
      <IDL_Type>double</IDL_Type>
      <Units>deg</Units>
      <Count>1</Count>
    </item>
    <item>
      <EFDB_Name>declination</EFDB_Name>
      <Description>Current pointing Declination</Description>
      <IDL_Type>double</IDL_Type>
      <Units>deg</Units>
      <Count>1</Count>
    </item>
    <item>
      <EFDB_Name>positionAngle</EFDB_Name>
      <Description>Current pointing sky position angle</Description>
      <IDL_Type>double</IDL_Type>
      <Units>deg</Units>
      <Count>1</Count>
    </item>
    <item>
      <EFDB_Name>parallacticAngle</EFDB_Name>
      <Description>Current parallactic angle.</Description>
      <IDL_Type>double</IDL_Type>
      <Units>deg</Units>
      <Count>1</Count>
    </item>
    <item>
      <EFDB_Name>tracking</EFDB_Name>
      <Description>Is the telescope tracking?</Description>
      <IDL_Type>boolean</IDL_Type>
      <Units>unitless</Units>
      <Count>1</Count>
    </item>
    <item>
      <EFDB_Name>telescopeAltitude</EFDB_Name>
      <Description>Telescope altitude.</Description>
      <IDL_Type>double</IDL_Type>
      <Units>deg</Units>
      <Count>1</Count>
    </item>
    <item>
      <EFDB_Name>telescopeAzimuth</EFDB_Name>
      <Description>telescope azimuth</Description>
      <IDL_Type>double</IDL_Type>
      <Units>deg</Units>
      <Count>1</Count>
    </item>
    <item>
      <EFDB_Name>telescopeRotator</EFDB_Name>
      <Description>telescope rotator position</Description>
      <IDL_Type>double</IDL_Type>
      <Units>deg</Units>
      <Count>1</Count>
    </item>
    <item>
      <EFDB_Name>domeAltitude</EFDB_Name>
      <Description>Dome altitude position</Description>
      <IDL_Type>double</IDL_Type>
      <Units>deg</Units>
      <Count>1</Count>
    </item>
    <item>
      <EFDB_Name>domeAzimuth</EFDB_Name>
      <Description>dome azimuth position</Description>
      <IDL_Type>double</IDL_Type>
      <Units>deg</Units>
      <Count>1</Count>
    </item>
    <item>
      <EFDB_Name>filterPosition</EFDB_Name>
      <Description>Current filter.</Description>
      <IDL_Type>string</IDL_Type>
      <IDL_Size>1</IDL_Size>
      <Units>unitless</Units>
      <Count>1</Count>
    </item>
    <item>
      <EFDB_Name>filterMounted</EFDB_Name>
      <Description>Current mounted/available filters.</Description>
      <IDL_Type>string</IDL_Type>
      <IDL_Size>1</IDL_Size>
      <Units>unitless</Units>
      <Count>1</Count>
    </item>
    <item>
      <EFDB_Name>filterUnmounted</EFDB_Name>
      <Description>Current unmounted/unavailable filters.</Description>
      <IDL_Type>string</IDL_Type>
      <IDL_Size>1</IDL_Size>
      <Units>unitless</Units>
      <Count>1</Count>
    </item>
  </SALTelemetry>
</SALTelemetrySet>
