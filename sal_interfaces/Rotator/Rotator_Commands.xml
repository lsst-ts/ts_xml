<SALCommand>
    <Subsystem>Rotator</Subsystem>
    <Version>3.5.1</Version>
    <Author></Author>
    <EFDB_Topic>Rotator_command_track</EFDB_Topic>
    <Alias>track</Alias>
    <Device></Device>
    <Property></Property>
    <Action></Action>
    <Value></Value>
    <Description>Rotator updates position and velocity setpoints to the values specified in the track command parameters. Successive track commands will arrive at a 20Hz rate.</Description>
    <item>
        <EFDB_Name>angle</EFDB_Name>
        <Description>Rotator absolute angle position in degrees (it could be a positive or a negative value)</Description>
        <IDL_Type>double</IDL_Type>
        <Units>deg</Units>
        <Count>1</Count>
    </item>
    <item>
        <EFDB_Name>velocity</EFDB_Name>
        <Description>Rotator velocity in degrees per second (positive or negative)</Description>
        <IDL_Type>double</IDL_Type>
        <Units>deg/s</Units>
        <Count>1</Count>
    </item>
    <item>
        <EFDB_Name>tai</EFDB_Name>
        <Description>Time in seconds</Description>
        <IDL_Type>double</IDL_Type>
        <Units>s</Units>
        <Count>1</Count>
    </item>
</SALCommand>
