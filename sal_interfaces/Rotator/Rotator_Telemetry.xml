<?xml version="1.0" encoding="UTF-8"?>
<?xml-stylesheet type="text/xsl" href="http://lsst-sal.tuc.noao.edu/schema/SALTelemetrySet.xsl"?>
<SALTelemetrySet xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:noNamespaceSchemaLocation="http://lsst-sal.tuc.noao.edu/schema/SALTelemetrySet.xsd">
<SALTelemetry>
    <Subsystem>Rotator</Subsystem>
    <Version>3.5.1</Version>
    <Author></Author>
    <EFDB_Topic>Rotator_Electrical</EFDB_Topic>
    <item>
        <EFDB_Name>CopleyStatusWordDrive</EFDB_Name>
        <Description>CopleyStatusWordDrive</Description>
        <IDL_Type>unsigned short</IDL_Type>
        <Units>unitless</Units>
        <Count>2</Count>
    </item>
    <item>
        <EFDB_Name>CopleyLatchingFaultStatus</EFDB_Name>
        <Description>CopleyLatchingFaultStatus</Description>
        <IDL_Type>unsigned short</IDL_Type>
        <Units>unitless</Units>
        <Count>2</Count>
    </item>
</SALTelemetry>
<SALTelemetry>
    <Subsystem>Rotator</Subsystem>
    <Version>3.5.1</Version>
    <Author></Author>
    <EFDB_Topic>Rotator_Application</EFDB_Topic>
    <item>
        <EFDB_Name>Demand</EFDB_Name>
        <Description>Commanded rotator position in degrees.</Description>
        <IDL_Type>double</IDL_Type>
        <Units>deg</Units>
        <Count>1</Count>
    </item>
    <item>
        <EFDB_Name>Position</EFDB_Name>
        <Description>Actual rotator position in degrees.</Description>
        <IDL_Type>double</IDL_Type>
        <Units>deg</Units>
        <Count>1</Count>
    </item>
    <item>
        <EFDB_Name>Error</EFDB_Name>
        <Description>Indication of a rotator following error.</Description>
        <IDL_Type>double</IDL_Type>
        <Units>deg</Units>
        <Count>1</Count>
    </item>
</SALTelemetry>
<SALTelemetry>
    <Subsystem>Rotator</Subsystem>
    <Version>3.5.1</Version>
    <Author></Author>
    <EFDB_Topic>Rotator_Motors</EFDB_Topic>
    <item>
        <EFDB_Name>Calibrated</EFDB_Name>
        <Description>Encoder readings from each rotator motor scaled to degrees of the rotator</Description>
        <IDL_Type>double</IDL_Type>
        <Units>deg</Units>
        <Count>2</Count>
    </item>
    <item>
        <EFDB_Name>Raw</EFDB_Name>
        <Description>Encoder readings from each rotator motor in counts</Description>
        <IDL_Type>double</IDL_Type>
        <Units>unitless</Units>
        <Count>2</Count>
    </item>
</SALTelemetry></SALTelemetrySet>