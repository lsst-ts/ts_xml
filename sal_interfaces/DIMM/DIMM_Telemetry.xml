        <IDL_Type>double</IDL_Type>
        <Units></Units>
        <Count>1</Count>
    </item>
</SALTelemetry>
<SALTelemetry>
    <Subsystem>DIMM</Subsystem>
    <Version>3.8.31</Version>
    <Author></Author>
    <EFDB_Topic>DIMM_loopTime</EFDB_Topic>
    <item>
        <EFDB_Name>loopTime</EFDB_Name>
        <Description>The actual loop time is milliseconds.</Description>
        <IDL_Type>double</IDL_Type>
        <Units></Units>
        <Count>1</Count>
    </item>
</SALTelemetry>
<SALTelemetry>
    <Subsystem>DIMM</Subsystem>
    <Version>3.8</Version>
    <Author>tribeiro</Author>
    <EFDB_Topic>DIMM_status</EFDB_Topic>
    <item>
        <EFDB_Name>status</EFDB_Name>
        <Description>The status of the DIMM.</Description>
        <IDL_Type>long</IDL_Type>
        <Units></Units>
        <Count>1</Count>
    </item>
    <item>
        <EFDB_Name>hrNum</EFDB_Name>
