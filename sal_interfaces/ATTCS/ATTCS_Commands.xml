<?xml-stylesheet type="text/xsl" href="http://lsst-sal.tuc.noao.edu/schema/SALCommandSet.xsl"?>
<SALCommandSet xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:noNamespaceSchemaLocation="http://lsst-sal.tuc.noao.edu/schema/SALCommandSet.xsd">
<SALCommand>
    <Subsystem>ATTCS</Subsystem>
    <Version>3.8.2</Version>
    <Author></Author>
    <EFDB_Topic>ATTCS_command_target</EFDB_Topic>
    <Alias>target</Alias>
    <Device></Device>
    <Property></Property>
    <Action></Action>
    <Value></Value>
    <Explanation>http://sal.lsst.org</Explanation>
    <item>
        <EFDB_Name>targetId</EFDB_Name>
        <Description>The id of the target.</Description>
        <IDL_Type>long</IDL_Type>
        <Units>unitless</Units>
        <Count>1</Count>
    </item>
    <item>
        <EFDB_Name>ra</EFDB_Name>
        <Description>The right acension of the target</Description>
        <IDL_Type>double</IDL_Type>
        <Units>arcsecond</Units>
        <Count>1</Count>
    </item>
    <item>
        <EFDB_Name>decl</EFDB_Name>
        <Description>The declination of the target.</Description>
        <IDL_Type>double</IDL_Type>
        <Units>degree</Units>
        <Count>1</Count>
    </item>
    <item>
        <EFDB_Name>skyPositionAngle</EFDB_Name>
        <Description>The sky position angle of the target.</Description>
        <IDL_Type>double</IDL_Type>
        <Units>degree</Units>
        <Count>1</Count>
    </item>
    <item>
        <EFDB_Name>numOfExposures</EFDB_Name>
        <Description>The number of exposures to take of the target.</Description>
        <IDL_Type>long</IDL_Type>
        <Units>unitless</Units>
        <Count>1</Count>
    </item>
    <item>
        <EFDB_Name>exposureTime</EFDB_Name>
        <Description>The exposure time for each exposure of the target.</Description>
        <IDL_Type>double</IDL_Type>
        <Units>second</Units>
        <Count>1</Count>
    </item>
</SALCommand>
<SALCommand>
    <Subsystem>ATTCS</Subsystem>
    <Version>3.8.2</Version>
    <Author></Author>
    <EFDB_Topic>ATTCS_command_offset</EFDB_Topic>
    <Alias>offset</Alias>
    <Device></Device>
    <Property></Property>
    <Action></Action>
    <Value></Value>
    <Explanation>http://sal.lsst.org</Explanation>
    <item>
        <EFDB_Name>offsetX</EFDB_Name>
        <Description>The offset in the x axis.</Description>
        <IDL_Type>double</IDL_Type>
        <Units>arcsecond</Units>
        <Count>1</Count>
    </item>
    <item>
        <EFDB_Name>offsetY</EFDB_Name>
        <Description>The offset in the y direction.</Description>
        <IDL_Type>double</IDL_Type>
        <Units>arcsecond</Units>
        <Count>1</Count>
    </item>
</SALCommand>
<SALCommand>
    <Subsystem>ATTCS</Subsystem>
    <Version>3.8.2</Version>
    <Author></Author>
    <EFDB_Topic>ATTCS_command_spectrographSetup</EFDB_Topic>
    <Alias>spectrographSetup</Alias>
    <Device></Device>
    <Property></Property>
    <Action></Action>
    <Value></Value>
    <Explanation>http://sal.lsst.org</Explanation>
            <item>
                <EFDB_Name>grating</EFDB_Name>
                <Description>The grating of the spectrograph to be setup.</Description>
                <!-- Enumeration: Grating -->
                <IDL_Type>long</IDL_Type>
                <Units>unitless</Units>
                <Count>1</Count>
            </item>
            <item>
                <EFDB_Name>filter</EFDB_Name>
                <Description>The filter of the spectrograph to be setup.</Description>
                <!-- Enumeration: Filter -->
                <IDL_Type>long</IDL_Type>
                <Units>unitless</Units>
                <Count>1</Count>
            </item>
    <item>
        <EFDB_Name>stagePosition</EFDB_Name>
        <Description>The position of the stage for the spectrograph to be set.</Description>
        <IDL_Type>double</IDL_Type>
        <Units>mm</Units>
        <Count>1</Count>
    </item>
</SALCommand>
</SALCommandSet>
