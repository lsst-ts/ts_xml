<?xml version="1.0" encoding="UTF-8"?>
<?xml-stylesheet type="text/xsl" href="http://lsst-sal.tuc.noao.edu/schema/SALEventSet.xsl"?>
<SALEventSet xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:noNamespaceSchemaLocation="http://lsst-sal.tuc.noao.edu/schema/SALEventSet.xsd">
<SALEvent>
    <Subsystem>DSM</Subsystem>
    <Version>4.0</Version>
    <Author></Author>
    <EFDB_Topic>DSM_logevent_settingsApplied</EFDB_Topic>
    <Alias>settingsApplied</Alias>
    <Explanation>http://sal.lsst.org</Explanation>
    <item>
        <EFDB_Name>telemetryDirectory</EFDB_Name>
        <Description>The location where the telemetry data from the UI is written. If it starts with /tmp, the CSC was run in simulation mode.</Description>
        <IDL_Type>string</IDL_Type>
        <Units>dimensionless</Units>
        <Count>256</Count>
    </item>
    <item>
        <EFDB_Name>simulationLoopTime</EFDB_Name>
        <Description>The sleep time between successive writes of telemetry data in simulation mode. If this is -1, the CSC is run in standard mode.</Description>
        <IDL_Type>long</IDL_Type>
        <Units>second</Units>
        <Count>1</Count>
    </item>
</SALEvent>
</SALEventSet>
