<?xml version="1.0" encoding="UTF-8"?>
<?xml-stylesheet type="text/xsl" href="http://lsst-sal.tuc.noao.edu/schema/SALEventSet.xsl"?>
<SALEventSet xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="http://lsst-sal.tuc.noao.edu/schema/SALEventSet.xsd">
  <SALEvent>
    <Subsystem>DSM</Subsystem>
    <EFDB_Topic>DSM_logevent_configuration</EFDB_Topic>
    <item>
      <EFDB_Name>dsmIndex</EFDB_Name>
      <Description>The index of the DSM unit.</Description>
      <IDL_Type>long</IDL_Type>
      <Units>unitless</Units>
      <Count>1</Count>
    </item>
    <item>
      <EFDB_Name>timestampConfigStart</EFDB_Name>
      <Description>The TAI timestamp for the start of the current configuration.</Description>
      <IDL_Type>double</IDL_Type>
      <Units>second</Units>
      <Count>1</Count>
    </item>
    <item>
      <EFDB_Name>uiVersionCode</EFDB_Name>
      <Description>The version of the DSM User Interface code.</Description>
      <IDL_Type>string</IDL_Type>
      <Units>unitless</Units>
      <Count>1</Count>
    </item>
    <item>
      <EFDB_Name>uiVersionConfig</EFDB_Name>
      <Description>The version of a Configuration passed to the DSM User Interface.</Description>
      <IDL_Type>string</IDL_Type>
      <Units>unitless</Units>
      <Count>1</Count>
    </item>
    <item>
      <EFDB_Name>uiConfigFile</EFDB_Name>
      <Description>A possible configuration file used by the DSM User Interface.</Description>
      <IDL_Type>string</IDL_Type>
      <Units>unitless</Units>
      <Count>1</Count>
    </item>
    <item>
      <EFDB_Name>cameraName</EFDB_Name>
      <Description>The type of camera used by the DSM User Interface.</Description>
      <IDL_Type>string</IDL_Type>
      <Units>unitless</Units>
      <Count>1</Count>
    </item>
    <item>
      <EFDB_Name>cameraFps</EFDB_Name>
      <Description>The Frames per Second rate of the camera.</Description>
      <IDL_Type>double</IDL_Type>
      <Units>1/second</Units>
      <Count>1</Count>
    </item>
    <item>
      <EFDB_Name>dataBufferSize</EFDB_Name>
      <Description>The size of the data buffer used in the calculations.</Description>
      <IDL_Type>long</IDL_Type>
      <Units>unitless</Units>
      <Count>1</Count>
    </item>
    <item>
      <EFDB_Name>dataAcquisitionTime</EFDB_Name>
      <Description>The total time is takes to fill a buffer for a calculation.</Description>
      <IDL_Type>double</IDL_Type>
      <Units>second</Units>
      <Count>1</Count>
    </item>
  </SALEvent>
</SALEventSet>
