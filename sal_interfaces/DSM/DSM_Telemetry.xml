<?xml version="1.0" encoding="UTF-8"?>
<?xml-stylesheet type="text/xsl" href="http://lsst-sal.tuc.noao.edu/schema/SALTelemetrySet.xsl"?>
<SALTelemetrySet xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="http://lsst-sal.tuc.noao.edu/schema/SALTelemetrySet.xsd">
  <SALTelemetry>
    <Subsystem>DSM</Subsystem>
    <Version>3.10</Version>
    <Author/>
    <EFDB_Topic>DSM_domeSeeing</EFDB_Topic>
    <item>
      <EFDB_Name>dsmIndex</EFDB_Name>
      <Description>The index of the DSM unit.</Description>
      <IDL_Type>long</IDL_Type>
      <Units>dimensionless</Units>
      <Count>1</Count>
    </item>
    <item>
      <EFDB_Name>timestampCurrent</EFDB_Name>
      <Description>The TAI timestamp of the current buffer measurement.</Description>
      <IDL_Type>double</IDL_Type>
      <Units>second</Units>
      <Count>1</Count>
    </item>
    <item>
      <EFDB_Name>timestampFirstMeasurement</EFDB_Name>
      <Description>The TAI timestamp of the first measurement in the buffer.</Description>
      <IDL_Type>double</IDL_Type>
      <Units>second</Units>
      <Count>1</Count>
    </item>
    <item>
      <EFDB_Name>timestampLastMeasurement</EFDB_Name>
      <Description>The TAI timestamp of the last measurement in the buffer.</Description>
      <IDL_Type>double</IDL_Type>
      <Units>second</Units>
      <Count>1</Count>
    </item>
    <item>
      <EFDB_Name>rmsX</EFDB_Name>
      <Description>The standard deviation of the buffer centroids in the X direction.</Description>
      <IDL_Type>double</IDL_Type>
      <Units>arcsec</Units>
      <Count>1</Count>
    </item>
    <item>
      <EFDB_Name>rmsY</EFDB_Name>
      <Description>The standard deviation of the buffer centroids in the Y direction.</Description>
      <IDL_Type>double</IDL_Type>
      <Units>arcsec</Units>
      <Count>1</Count>
    </item>
    <item>
      <EFDB_Name>centroidX</EFDB_Name>
      <Description>The average spot position of the buffer centroids in the X direction.</Description>
      <IDL_Type>double</IDL_Type>
      <Units>pixel</Units>
      <Count>1</Count>
    </item>
    <item>
      <EFDB_Name>centroidY</EFDB_Name>
      <Description>The average spot position of the buffer centroids in the Y direction.</Description>
      <IDL_Type>double</IDL_Type>
      <Units>pixel</Units>
      <Count>1</Count>
    </item>
    <item>
      <EFDB_Name>flux</EFDB_Name>
      <Description>The average of the frame fluxes (ADU) in the buffer tracking the centroids.</Description>
      <IDL_Type>double</IDL_Type>
      <Units>unitless</Units>
      <Count>1</Count>
    </item>
    <item>
      <EFDB_Name>maxADC</EFDB_Name>
      <Description>The average of the frame maximum ADC values in the buffer tracking the centroids.</Description>
      <IDL_Type>double</IDL_Type>
      <Units>unitless</Units>
      <Count>1</Count>
    </item>
    <item>
      <EFDB_Name>fwhm</EFDB_Name>
      <Description>The average of the spot full-width, half-maxes in the buffer tracking the centroids.</Description>
      <IDL_Type>double</IDL_Type>
      <Units>pixel</Units>
      <Count>1</Count>
    </item>
  </SALTelemetry>
  <SALTelemetry>
    <Subsystem>DSM</Subsystem>
    <Version>3.10</Version>
    <Author>Michael Reuter</Author>
    <EFDB_Topic>DSM_configuration</EFDB_Topic>
    <item>
      <EFDB_Name>dsmIndex</EFDB_Name>
      <Description>The index of the DSM unit.</Description>
      <IDL_Type>long</IDL_Type>
      <Units>dimensionless</Units>
      <Count>1</Count>
    </item>
    <item>
      <EFDB_Name>timestampConfigStart</EFDB_Name>
      <Description>The TAI timestamp for the start of the current configuration.</Description>
      <IDL_Type>double</IDL_Type>
      <Units>second</Units>
      <Count>1</Count>
    </item>
    <item>
      <EFDB_Name>uiVersionCode</EFDB_Name>
      <Description>The version of the DSM User Interface code.</Description>
      <IDL_Type>string</IDL_Type>
      <Units>dimensionless</Units>
      <Count>16</Count>
    </item>
    <item>
      <EFDB_Name>uiVersionConfig</EFDB_Name>
      <Description>The version of a Configuration passed to the DSM User Interface.</Description>
      <IDL_Type>string</IDL_Type>
      <Units>dimensionless</Units>
      <Count>16</Count>
    </item>
    <item>
      <EFDB_Name>uiConfigFile</EFDB_Name>
      <Description>A possible configuration file used by the DSM User Interface.</Description>
      <IDL_Type>string</IDL_Type>
      <Units>dimensionless</Units>
      <Count>256</Count>
    </item>
    <item>
      <EFDB_Name>cameraName</EFDB_Name>
      <Description>The type of camera used by the DSM User Interface.</Description>
      <IDL_Type>string</IDL_Type>
      <Units>dimensionless</Units>
      <Count>16</Count>
    </item>
    <item>
      <EFDB_Name>cameraFps</EFDB_Name>
      <Description>The Frames per Second rate of the camera.</Description>
      <IDL_Type>double</IDL_Type>
      <Units>1/second</Units>
      <Count>1</Count>
    </item>
    <item>
      <EFDB_Name>dataBufferSize</EFDB_Name>
      <Description>The size of the data buffer used in the calculations.</Description>
      <IDL_Type>long</IDL_Type>
      <Units>dimensionless</Units>
      <Count>1</Count>
    </item>
    <item>
      <EFDB_Name>dataAcquisitionTime</EFDB_Name>
      <Description>The total time is takes to fill a buffer for a calculation.</Description>
      <IDL_Type>double</IDL_Type>
      <Units>second</Units>
      <Count>1</Count>
    </item>
  </SALTelemetry>
</SALTelemetrySet>
