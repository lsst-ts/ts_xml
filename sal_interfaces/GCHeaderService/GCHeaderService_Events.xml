      <Count>1</Count>
    </item>
    <item>
      <EFDB_Name>id</EFDB_Name>
      <Description>The Unique ID for the Large File Object</Description>
      <IDL_Type>string</IDL_Type>
      <IDL_Size>256</IDL_Size>
      <Units>unitless</Units>
      <Count>1</Count>
