<?xml version="1.0" encoding="UTF-8"?>
<?xml-stylesheet type="text/xsl" href="http://lsst-sal.tuc.noao.edu/schema/SALTelemetrySet.xsl"?>
<SALTelemetrySet xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:noNamespaceSchemaLocation="http://lsst-sal.tuc.noao.edu/schema/SALTelemetrySet.xsd">
<SALTelemetry>
    <Subsystem>Hexapod</Subsystem>
    <Version>3.5.1</Version>
    <Author></Author>
    <EFDB_Topic>Hexapod_Actuators</EFDB_Topic>
    <item>
        <EFDB_Name>Calibrated</EFDB_Name>
        <Description>Linear encoder readings from each hexapod actuator (x1,x2,x3,x4,x5,x6) in microns</Description>
        <IDL_Type>double</IDL_Type>
        <Units>um</Units>
        <Count>6</Count>
    </item>
    <item>
        <EFDB_Name>Raw</EFDB_Name>
        <Description>Linear encoder readings from each hexapod actuator (x1,x2,x3,x4,x5,x6)in counts</Description>
        <IDL_Type>double</IDL_Type>
        <Units>Dimensionless</Units>
        <Count>6</Count>
    </item>
</SALTelemetry>
<SALTelemetry>
    <Subsystem>Hexapod</Subsystem>
    <Version>3.5.1</Version>
    <Author></Author>
    <EFDB_Topic>Hexapod_Application</EFDB_Topic>
    <item>
        <EFDB_Name>Demand</EFDB_Name>
        <Description>Commanded hexapod position in the following order (X, Y, Z, RX, RY, RZ). Linear positions are in microns, angular positions are in degrees.</Description>
        <IDL_Type>double</IDL_Type>
        <Units>Dimensionless</Units>
        <Count>6</Count>
    </item>
    <item>
        <EFDB_Name>Error</EFDB_Name>
        <Description>Indication of an actuator following error (x1,x2,x3,x4,x5,x6) if there is no error shall be 0.</Description>
        <IDL_Type>double</IDL_Type>
        <Units>Dimensionless</Units>
        <Count>6</Count>
    </item>
    <item>
        <EFDB_Name>Position</EFDB_Name>
        <Description>Actual hexapod position in the following order (X, Y, Z, RX, RY, RZ). Linear positions are in microns, angular positions are in degrees.</Description>
        <IDL_Type>double</IDL_Type>
        <Units>Dimensionless</Units>
        <Count>6</Count>
    </item>
</SALTelemetry>
<SALTelemetry>
    <Subsystem>Hexapod</Subsystem>
    <Version>3.5.1</Version>
    <Author></Author>
    <EFDB_Topic>Hexapod_Electrical</EFDB_Topic>
    <item>
        <EFDB_Name>CopleyStatusWordDrive</EFDB_Name>
        <Description>Bus undervoltage fault, bus overvoltage fault, and overcurrent fault are all being monitored for each drive channel</Description>
        <IDL_Type>unsigned short</IDL_Type>
        <Units>Dimensionless</Units>
        <Count>6</Count>
    </item>
    <item>
        <EFDB_Name>CopleyLatchingFaultStatus</EFDB_Name>
        <Description>CopleyLatchingFaultStatus</Description>
        <IDL_Type>unsigned short</IDL_Type>
        <Units>Dimensionless</Units>
        <Count>6</Count>
    </item>
</SALTelemetry></SALTelemetrySet>
