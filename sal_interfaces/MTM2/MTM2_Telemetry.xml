<?xml version="1.0" encoding="UTF-8"?>
<?xml-stylesheet type="text/xsl" href="http://lsst-sal.tuc.noao.edu/schema/SALTelemetrySet.xsl"?>
<SALTelemetrySet xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:noNamespaceSchemaLocation="http://lsst-sal.tuc.noao.edu/schema/SALTelemetrySet.xsd">

<SALTelemetry>
    <Subsystem>MTM2</Subsystem>
    <EFDB_Topic>MTM2_mirrorPositionMeasured</EFDB_Topic>
    <Description>M2 mirror's rigid body position.</Description>
    <item>
        <EFDB_Name>x</EFDB_Name>
        <Description>Position x.</Description>
        <IDL_Type>double</IDL_Type>
        <Units>micron</Units>
        <Count>1</Count>
    </item>
    <item>
        <EFDB_Name>y</EFDB_Name>
        <Description>Position y.</Description>
        <IDL_Type>double</IDL_Type>
        <Units>micron</Units>
        <Count>1</Count>
    </item>
    <item>
        <EFDB_Name>z</EFDB_Name>
        <Description>Position z.</Description>
        <IDL_Type>double</IDL_Type>
        <Units>micron</Units>
        <Count>1</Count>
    </item>
    <item>
        <EFDB_Name>xRot</EFDB_Name>
        <Description>Rotation about x.</Description>
        <IDL_Type>double</IDL_Type>
        <Units>arcsec</Units>
        <Count>1</Count>
    </item>
    <item>
        <EFDB_Name>yRot</EFDB_Name>
        <Description>Rotation about y.</Description>
        <IDL_Type>double</IDL_Type>
        <Units>arcsec</Units>
        <Count>1</Count>
    </item>
    <item>
        <EFDB_Name>zRot</EFDB_Name>
        <Description>Rotation about z.</Description>
        <IDL_Type>double</IDL_Type>
        <Units>arcsec</Units>
        <Count>1</Count>
    </item>
</SALTelemetry>

<SALTelemetry>
    <Subsystem>MTM2</Subsystem>
    <EFDB_Topic>MTM2_mirrorPositionMeasuredIMS</EFDB_Topic>
    <Description>M2 mirror's rigid body position measured by the independent measurement system (IMS).</Description>
    <item>
        <EFDB_Name>x</EFDB_Name>
        <Description>Position x.</Description>
        <IDL_Type>double</IDL_Type>
        <Units>micron</Units>
        <Count>1</Count>
    </item>
    <item>
        <EFDB_Name>y</EFDB_Name>
        <Description>Position y.</Description>
        <IDL_Type>double</IDL_Type>
        <Units>micron</Units>
        <Count>1</Count>
    </item>
    <item>
        <EFDB_Name>z</EFDB_Name>
        <Description>Position z.</Description>
        <IDL_Type>double</IDL_Type>
        <Units>micron</Units>
        <Count>1</Count>
    </item>
    <item>
        <EFDB_Name>xRot</EFDB_Name>
        <Description>Rotation about x.</Description>
        <IDL_Type>double</IDL_Type>
        <Units>arcsec</Units>
        <Count>1</Count>
    </item>
    <item>
        <EFDB_Name>yRot</EFDB_Name>
        <Description>Rotation about y.</Description>
        <IDL_Type>double</IDL_Type>
        <Units>arcsec</Units>
        <Count>1</Count>
    </item>
    <item>
        <EFDB_Name>zRot</EFDB_Name>
        <Description>Rotation about z.</Description>
        <IDL_Type>double</IDL_Type>
        <Units>arcsec</Units>
        <Count>1</Count>
    </item>
</SALTelemetry>

<SALTelemetry>
    <Subsystem>MTM2</Subsystem>
    <EFDB_Topic>MTM2_axialForceData</EFDB_Topic>
    <Description>Axial actuator forces data. Increasing force corresponds to increasing tension in the load cell. The elements are the actuator Ids from 1 to 72. The related ring Ids are B1-B30, C1-C24, and D1-D18.</Description>
    <item>
        <EFDB_Name>axialForcesLutGravity</EFDB_Name>
        <Description>Gravity component (F_e + F_0 + F_a + F_f) of look-up table (LUT) force for each actuator in sequence.</Description>
        <IDL_Type>double</IDL_Type>
        <Units>newton</Units>
        <Count>72</Count>
    </item>
    <item>
        <EFDB_Name>axialForcesLutTemperature</EFDB_Name>
        <Description>Temperature component (T_u + T_x + T_y + T_r) of look-up table (LUT) force for each actuator in sequence.</Description>
        <IDL_Type>double</IDL_Type>
        <Units>newton</Units>
        <Count>72</Count>
    </item>
    <item>
        <EFDB_Name>axialForcesApplied</EFDB_Name>
        <Description>Force applied by SAL command or script for each actuator in sequence.</Description>
        <IDL_Type>double</IDL_Type>
        <Units>newton</Units>
        <Count>72</Count>
    </item>
    <item>
        <EFDB_Name>axialForcesMeasured</EFDB_Name>
        <Description>Force measurement by load cell for each actuator in sequence.</Description>
        <IDL_Type>double</IDL_Type>
        <Units>newton</Units>
        <Count>72</Count>
    </item>
    <item>
        <EFDB_Name>axialForcesHardpointCorrection</EFDB_Name>
        <Description>Hardpoint compensation force correction for each actuator in sequence.</Description>
        <IDL_Type>double</IDL_Type>
        <Units>newton</Units>
        <Count>72</Count>
    </item>
</SALTelemetry>

<SALTelemetry>
    <Subsystem>MTM2</Subsystem>
    <EFDB_Topic>MTM2_tangentForceData</EFDB_Topic>
    <Description>Tangent actuator forces data. Increasing force corresponds to increasing tension in the load cell. The elements are the actuator Ids from 73 to 78. The related ring Ids are A1-A6. The related degrees are 0, 60, 120, 180, 240, and 300.</Description>
    <item>
        <EFDB_Name>tangentForcesLutGravity</EFDB_Name>
        <Description>Gravity component (F_e + F_0 + F_a + F_f) of look-up table (LUT) force for each actuator in sequence.</Description>
        <IDL_Type>double</IDL_Type>
        <Units>newton</Units>
        <Count>6</Count>
    </item>
    <item>
        <EFDB_Name>tangentForcesLutTemperature</EFDB_Name>
        <Description>Temperature component (T_u + T_x + T_y + T_r) of look-up table (LUT) force for each actuator in sequence.</Description>
        <IDL_Type>double</IDL_Type>
        <Units>newton</Units>
        <Count>6</Count>
    </item>
    <item>
        <EFDB_Name>tangentForcesApplied</EFDB_Name>
        <Description>Force applied by SAL command or script for each actuator in sequence.</Description>
        <IDL_Type>double</IDL_Type>
        <Units>newton</Units>
        <Count>6</Count>
    </item>
    <item>
        <EFDB_Name>tangentForcesMeasured</EFDB_Name>
        <Description>Force measurement by load cell for each actuator in sequence.</Description>
        <IDL_Type>double</IDL_Type>
        <Units>newton</Units>
        <Count>6</Count>
    </item>
    <item>
        <EFDB_Name>tangentForcesHardpointCorrection</EFDB_Name>
        <Description>Hardpoint compensation force correction for each actuator in sequence.</Description>
        <IDL_Type>double</IDL_Type>
        <Units>newton</Units>
        <Count>6</Count>
    </item>
</SALTelemetry>

<SALTelemetry>
    <Subsystem>MTM2</Subsystem>
    <EFDB_Topic>MTM2_temperaturesMeasured</EFDB_Topic>
    <Description>Mirror and cell temperature sensor readings relative to the origin (offset).</Description>
    <item>
        <EFDB_Name>ringTemperature</EFDB_Name>
        <Description>Ring temperatures: LG2-1, LG2-2, LG2-3, LG2-4, LG3-1, LG3-2, LG3-3, LG3-4, LG4-1, LG4-2, LG4-3, and LG4-4.</Description>
        <IDL_Type>double</IDL_Type>
        <Units>deg_C</Units>
        <Count>12</Count>
    </item>
    <item>
        <EFDB_Name>intakeTemperature</EFDB_Name>
        <Description>Intake temperatures: #1 and #2.</Description>
        <IDL_Type>double</IDL_Type>
        <Units>deg_C</Units>
        <Count>2</Count>
    </item>
    <item>
        <EFDB_Name>exhaustTemperature</EFDB_Name>
        <Description>Exhaust temperatures: #1 and #2.</Description>
        <IDL_Type>double</IDL_Type>
        <Units>deg_C</Units>
        <Count>2</Count>
    </item>
</SALTelemetry>

<SALTelemetry>
    <Subsystem>MTM2</Subsystem>
    <EFDB_Topic>MTM2_zenithAngleData</EFDB_Topic>
    <Description>M2 internal inclinometer reading transforms to the zenith angle.</Description>
    <item>
        <EFDB_Name>zenithAngleMeasured</EFDB_Name>
        <Description>Zenith angle is 0 degree when the telescope is at zenith.</Description>
        <IDL_Type>double</IDL_Type>
        <Units>degree</Units>
        <Count>1</Count>
    </item>
    <item>
        <EFDB_Name>rawAngleInclinometer</EFDB_Name>
        <Description>Reading raw angle of inclinometer (0-360 degree).</Description>
        <IDL_Type>double</IDL_Type>
        <Units>degree</Units>
        <Count>1</Count>
    </item>
    <item>
        <EFDB_Name>procAngleInclinometer</EFDB_Name>
        <Description>Processed angle of inclinometer (0-360 degree).</Description>
        <IDL_Type>double</IDL_Type>
        <Units>degree</Units>
        <Count>1</Count>
    </item>
</SALTelemetry>

<SALTelemetry>
    <Subsystem>MTM2</Subsystem>
    <EFDB_Topic>MTM2_axialActuatorSteps</EFDB_Topic>
    <Description>Axial actuator stepper motor position in steps. Increasing step corresponds to counterclockwise rotation of the motor shaft, looking along the shaft in the direction of the motor. The elements are the actuator Ids from 1 to 72. The related ring Ids are B1-B30, C1-C24, and D1-D18.</Description>
    <item>
        <EFDB_Name>axialActuatorSteps</EFDB_Name>
        <Description>Array of steps for each axial actuator in sequence.</Description>
        <IDL_Type>long</IDL_Type>
        <Units>unitless</Units>
        <Count>72</Count>
    </item>
</SALTelemetry>

<SALTelemetry>
    <Subsystem>MTM2</Subsystem>
    <EFDB_Topic>MTM2_tangentActuatorSteps</EFDB_Topic>
    <Description>Tangent actuator stepper motor position in steps. Increasing sign indicates extension towards the mirror. The elements are the actuator Ids from 73 to 78. The related ring Ids are A1-A6. The related degrees are 0, 60, 120, 180, 240, and 300.</Description>
    <item>
        <EFDB_Name>tangentActuatorSteps</EFDB_Name>
        <Description>Array of steps for each actuator in sequence.</Description>
        <IDL_Type>long</IDL_Type>
        <Units>unitless</Units>
        <Count>6</Count>
    </item>
</SALTelemetry>

<SALTelemetry>
    <Subsystem>MTM2</Subsystem>
    <EFDB_Topic>MTM2_axialEncoderPositions</EFDB_Topic>
    <Description>Axial actuator encoder position. The sign of the position is consistent with the optical coordinate system as per LTS-136. The elements are the actuator Ids from 1 to 72. The related ring Ids are B1-B30, C1-C24, and D1-D18.</Description>
    <item>
        <EFDB_Name>axialEncoderPositions</EFDB_Name>
        <Description>Encoder position of each axial encoder in sequence.</Description>
        <IDL_Type>double</IDL_Type>
        <Units>micron</Units>
        <Count>72</Count>
    </item>
</SALTelemetry>

<SALTelemetry>
    <Subsystem>MTM2</Subsystem>
    <EFDB_Topic>MTM2_tangentEncoderPositions</EFDB_Topic>
    <Description>Tangent actuator encoder position. The sign of the position is consistent with the optical coordinate system as per LTS-136. The elements are the actuator Ids from 73 to 78. The related ring Ids are A1-A6. The related degrees are 0, 60, 120, 180, 240, and 300.</Description>
    <item>
        <EFDB_Name>tangentEncoderPositions</EFDB_Name>
        <Description>Encoder position of each tangent encoder in sequence.</Description>
        <IDL_Type>double</IDL_Type>
        <Units>micron</Units>
        <Count>6</Count>
    </item>
</SALTelemetry>

<SALTelemetry>
    <Subsystem>MTM2</Subsystem>
    <EFDB_Topic>MTM2_ilcData</EFDB_Topic>
    <Description>Inner-loop controller (ILC) data.</Description>
    <item>
        <EFDB_Name>status</EFDB_Name>
        <Description>ILC status. The elements are the actuator Ids from 1 to 78. The value needs to be transformed from decimal to hexadecimal to understand. The details are in code 67, LTS-346.</Description>
        <IDL_Type>unsigned short</IDL_Type>
        <Units>unitless</Units>
        <Count>78</Count>
    </item>
</SALTelemetry>

<SALTelemetry>
    <Subsystem>MTM2</Subsystem>
    <EFDB_Topic>MTM2_displacementSensors</EFDB_Topic>
    <Description>Raw measurements from displacement sensors.</Description>
    <item>
        <EFDB_Name>thetaZ</EFDB_Name>
        <Description>Readings of theta-z from displacement sensors. The elements are A1, A2, A3, A4, A5, and A6.</Description>
        <IDL_Type>double</IDL_Type>
        <Units>micron</Units>
        <Count>6</Count>
    </item>
    <item>
        <EFDB_Name>deltaZ</EFDB_Name>
        <Description>Readings of delta-z from displacement sensors. The elements are A1, A2, A3, A4, A5, and A6.</Description>
        <IDL_Type>double</IDL_Type>
        <Units>micron</Units>
        <Count>6</Count>
    </item>
</SALTelemetry>

<SALTelemetry>
    <Subsystem>MTM2</Subsystem>
    <EFDB_Topic>MTM2_balanceForceData</EFDB_Topic>
    <Description>Net forces and moments as commanded by the force balance system. These are calculated using the 72 axial and 6 tangent balance forces, which are a component of the total forces being commanded.</Description>
    <item>
        <EFDB_Name>fx</EFDB_Name>
        <Description>Total x-force commanded by the force balance system.</Description>
        <IDL_Type>double</IDL_Type>
        <Units>newton</Units>
        <Count>1</Count>
    </item>
    <item>
        <EFDB_Name>fy</EFDB_Name>
        <Description>Total y-force commanded by the force balance system.</Description>
        <IDL_Type>double</IDL_Type>
        <Units>newton</Units>
        <Count>1</Count>
    </item>
    <item>
        <EFDB_Name>fz</EFDB_Name>
        <Description>Total z-force commanded by the force balance system.</Description>
        <IDL_Type>double</IDL_Type>
        <Units>newton</Units>
        <Count>1</Count>
    </item>
    <item>
        <EFDB_Name>mx</EFDB_Name>
        <Description>Total x-moment of force commanded by the force balance system.</Description>
        <IDL_Type>double</IDL_Type>
        <Units>newton*meter</Units>
        <Count>1</Count>
    </item>
    <item>
        <EFDB_Name>my</EFDB_Name>
        <Description>Total y-moment of force commanded by the force balance system.</Description>
        <IDL_Type>double</IDL_Type>
        <Units>newton*meter</Units>
        <Count>1</Count>
    </item>
    <item>
        <EFDB_Name>mz</EFDB_Name>
        <Description>Total z-moment of force commanded by the force balance system.</Description>
        <IDL_Type>double</IDL_Type>
        <Units>newton*meter</Units>
        <Count>1</Count>
    </item>
</SALTelemetry>

<SALTelemetry>
    <Subsystem>MTM2</Subsystem>
    <EFDB_Topic>MTM2_netForcesTotal</EFDB_Topic>
    <Description>Totol actuator net forces.</Description>
    <item>
        <EFDB_Name>fx</EFDB_Name>
        <Description>Net force in x direction.</Description>
        <IDL_Type>double</IDL_Type>
        <Units>newton</Units>
        <Count>1</Count>
    </item>
    <item>
        <EFDB_Name>fy</EFDB_Name>
        <Description>Net force in y direction.</Description>
        <IDL_Type>double</IDL_Type>
        <Units>newton</Units>
        <Count>1</Count>
    </item>
    <item>
        <EFDB_Name>fz</EFDB_Name>
        <Description>Net force in z direction.</Description>
        <IDL_Type>double</IDL_Type>
        <Units>newton</Units>
        <Count>1</Count>
    </item>
</SALTelemetry>

<SALTelemetry>
    <Subsystem>MTM2</Subsystem>
    <EFDB_Topic>MTM2_netMomentsTotal</EFDB_Topic>
    <Description>Totol actuator net moments of force.</Description>
    <item>
        <EFDB_Name>mx</EFDB_Name>
        <Description>Net moment of force in x direction.</Description>
        <IDL_Type>double</IDL_Type>
        <Units>newton*meter</Units>
        <Count>1</Count>
    </item>
    <item>
        <EFDB_Name>my</EFDB_Name>
        <Description>Net moment of force in y direction.</Description>
        <IDL_Type>double</IDL_Type>
        <Units>newton*meter</Units>
        <Count>1</Count>
    </item>
    <item>
        <EFDB_Name>mz</EFDB_Name>
        <Description>Net moment of force in z direction.</Description>
        <IDL_Type>double</IDL_Type>
        <Units>newton*meter</Units>
        <Count>1</Count>
    </item>
</SALTelemetry>

<SALTelemetry>
    <Subsystem>MTM2</Subsystem>
    <EFDB_Topic>MTM2_powerStatus</EFDB_Topic>
    <Description>Total voltage and current for motors and communication system of M2.</Description>
    <item>
        <EFDB_Name>motorVoltage</EFDB_Name>
        <Description>Total motor voltage.</Description>
        <IDL_Type>double</IDL_Type>
        <Units>volt</Units>
        <Count>1</Count>
    </item>
    <item>
        <EFDB_Name>motorCurrent</EFDB_Name>
        <Description>Total motor current.</Description>
        <IDL_Type>double</IDL_Type>
        <Units>ampere</Units>
        <Count>1</Count>
    </item>
    <item>
        <EFDB_Name>commVoltage</EFDB_Name>
        <Description>Total communication voltage.</Description>
        <IDL_Type>double</IDL_Type>
        <Units>volt</Units>
        <Count>1</Count>
    </item>
    <item>
        <EFDB_Name>commCurrent</EFDB_Name>
        <Description>Total communication current.</Description>
        <IDL_Type>double</IDL_Type>
        <Units>ampere</Units>
        <Count>1</Count>
    </item>
</SALTelemetry>

</SALTelemetrySet>
