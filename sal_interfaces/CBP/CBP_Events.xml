<?xml version="1.0" encoding="utf-8"?>
<SALEventSet xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="https://github.com/lsst-ts/ts_xml/blob/develop/schema/SALEventSet.xsd">
  <SALEvent>
    <Subsystem>CBP</Subsystem>
    <EFDB_Topic>CBP_logevent_target</EFDB_Topic>
    <Description>Reports the commanded movement.</Description>
    <item>
      <EFDB_Name>elevation</EFDB_Name>
      <Description>The commanded elevation</Description>
      <IDL_Type>double</IDL_Type>
      <Units>degree</Units>
      <Count>1</Count>
    </item>
    <item>
      <EFDB_Name>azimuth</EFDB_Name>
      <Description>The commanded azimuth</Description>
      <IDL_Type>double</IDL_Type>
      <Units>degree</Units>
      <Count>1</Count>
    </item>
    <item>
      <EFDB_Name>focus</EFDB_Name>
      <Description>The commanded focus</Description>
      <IDL_Type>double</IDL_Type>
      <Units>millimeter</Units>
      <Count>1</Count>
    </item>
    <item>
      <EFDB_Name>mask</EFDB_Name>
      <Description>The commanded mask</Description>
      <IDL_Type>string</IDL_Type>
      <Units>unitless</Units>
      <Count>1</Count>
    </item>
    <item>
      <EFDB_Name>mask_rotation</EFDB_Name>
      <Description>The commanded mask rotation</Description>
      <IDL_Type>double</IDL_Type>
      <Units>degree</Units>
      <Count>1</Count>
    </item>
  </SALEvent>
  <SALEvent>
    <Subsystem>CBP</Subsystem>
    <EFDB_Topic>CBP_logevent_inPosition</EFDB_Topic>
    <Description>Reports the motion status of the encoders</Description>
    <item>
      <EFDB_Name>elevation</EFDB_Name>
      <Description>The motion status for elevation</Description>
      <IDL_Type>boolean</IDL_Type>
      <Units>unitless</Units>
      <Count>1</Count>
    </item>
    <item>
      <EFDB_Name>azimuth</EFDB_Name>
      <Description>The motion status for azimuth</Description>
      <IDL_Type>boolean</IDL_Type>
      <Units>unitless</Units>
      <Count>1</Count>
    </item>
    <item>
      <EFDB_Name>focus</EFDB_Name>
      <Description>The motion status for focus</Description>
      <IDL_Type>boolean</IDL_Type>
      <Units>unitless</Units>
      <Count>1</Count>
    </item>
    <item>
      <EFDB_Name>mask</EFDB_Name>
      <Description>The motion status for mask</Description>
      <IDL_Type>boolean</IDL_Type>
      <Units>unitless</Units>
      <Count>1</Count>
    </item>
    <item>
      <EFDB_Name>mask_rotation</EFDB_Name>
      <Description>The motion status for mask rotation</Description>
      <IDL_Type>boolean</IDL_Type>
      <Units>unitless</Units>
      <Count>1</Count>
    </item>
  </SALEvent>
</SALEventSet>
