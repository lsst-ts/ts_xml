<?xml version="1.0" encoding="UTF-8"?>
<?xml-stylesheet type="text/xsl" href="http://lsst-sal.tuc.noao.edu/schema/SALCommandSet.xsl"?>
<SALCommandSet xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="http://lsst-sal.tuc.noao.edu/schema/SALCommandSet.xsd">
    <SALCommand>
        <Subsystem>Dome</Subsystem>
        <EFDB_Topic>Dome_command_crawlAz</EFDB_Topic>
        <Description>Move the azimuth axis at constant velocity.</Description>
        <item>
            <EFDB_Name>velocity</EFDB_Name>
            <Description>The velocity. Positive is the direction of increasing azimuth.</Description>
            <IDL_Type>double</IDL_Type>
            <Units>deg/second</Units>
            <Count>1</Count>
        </item>
    </SALCommand>
    <SALCommand>
        <Subsystem>Dome</Subsystem>
        <EFDB_Topic>Dome_command_crawlEl</EFDB_Topic>
        <Description>Move the elevation axis (light/wind screen) at constant velocity.</Description>
        <item>
            <EFDB_Name>velocity</EFDB_Name>
            <Description>The velocity. Positive is the direction of increasing azimuth.</Description>
            <IDL_Type>double</IDL_Type>
            <Units>deg/second</Units>
            <Count>1</Count>
        </item>
    </SALCommand>
    <SALCommand>
        <Subsystem>Dome</Subsystem>
        <EFDB_Topic>Dome_command_moveAz</EFDB_Topic>
        <Description>Move the azimuth axis at constant velocity, starting from a specified position.</Description>
        <item>
            <EFDB_Name>position</EFDB_Name>
            <Description>The position.</Description>
            <IDL_Type>double</IDL_Type>
            <Units>deg</Units>
            <Count>1</Count>
        </item>
        <item>
            <EFDB_Name>velocity</EFDB_Name>
            <Description>The velocity. Positive is the direction of increasing azimuth.</Description>
            <IDL_Type>double</IDL_Type>
            <Units>deg/second</Units>
            <Count>1</Count>
        </item>
    </SALCommand>
    <SALCommand>
        <Subsystem>Dome</Subsystem>
        <EFDB_Topic>Dome_command_moveEl</EFDB_Topic>
        <Description>Move the elevation axis (light/wind screen) to a specified position.</Description>
        <item>
            <EFDB_Name>position</EFDB_Name>
            <Description>The position.</Description>
            <IDL_Type>double</IDL_Type>
            <Units>deg</Units>
            <Count>1</Count>
        </item>
    </SALCommand>
    <SALCommand>
        <Subsystem>Dome</Subsystem>
        <EFDB_Topic>Dome_command_park</EFDB_Topic>
    </SALCommand>
    <SALCommand>
        <Subsystem>Dome</Subsystem>
        <EFDB_Topic>Dome_command_setLouvers</EFDB_Topic>
        <Description>Move one or more louvers. The Louver enumeration describes the louver indices.</Description>
        <item>
            <EFDB_Name>position</EFDB_Name>
            <Description>Desired percent open of each louver: 0 is fully closed, 100 is fully open, -1 is don't move.</Description>
            <IDL_Type>float</IDL_Type>
            <Units>unitless</Units>
            <Count>34</Count>
        </item>
    </SALCommand>
    <SALCommand>
        <Subsystem>Dome</Subsystem>
        <EFDB_Topic>Dome_command_closeLouvers</EFDB_Topic>
        <Description>Close all louvers.</Description>
    </SALCommand>
    <SALCommand>
        <Subsystem>Dome</Subsystem>
        <EFDB_Topic>Dome_command_stopLouvers</EFDB_Topic>
        <Description>Stop all louvers.</Description>
    </SALCommand>
    <SALCommand>
        <Subsystem>Dome</Subsystem>
        <EFDB_Topic>Dome_command_closeShutter</EFDB_Topic>
        <Description>Close the shutter.</Description>
    </SALCommand>
    <SALCommand>
        <Subsystem>Dome</Subsystem>
        <EFDB_Topic>Dome_command_openShutter</EFDB_Topic>
        <Description>Open the shutter.</Description>
    </SALCommand>
    <SALCommand>
        <Subsystem>Dome</Subsystem>
        <EFDB_Topic>Dome_command_stopShutter</EFDB_Topic>
        <Description>Stop the shutter.</Description>
    </SALCommand>
    <SALCommand>
        <Subsystem>Dome</Subsystem>
        <EFDB_Topic>Dome_command_stopAz</EFDB_Topic>
        <Description>Stop azimuth motion.</Description>
    </SALCommand>
    <SALCommand>
        <Subsystem>Dome</Subsystem>
        <EFDB_Topic>Dome_command_stopEl</EFDB_Topic>
        <Description>Stop elevation (light/wind screen) motion.</Description>
    </SALCommand>
    <SALCommand>
        <Subsystem>Dome</Subsystem>
        <EFDB_Topic>Dome_command_stop</EFDB_Topic>
        <Description>Stop all motion.</Description>
    </SALCommand>
    <SALCommand>
        <Subsystem>Dome</Subsystem>
        <EFDB_Topic>Dome_command_setTemperature</EFDB_Topic>
        <Description>Set the desired temperature of the dome heat sources (motors and cabinets)</Description>
        <item>
            <EFDB_Name>temperature</EFDB_Name>
            <Description>Desired temperature.</Description>
            <IDL_Type>double</IDL_Type>
            <Units>deg_C</Units>
            <Count>1</Count>
        </item>
    </SALCommand>
</SALCommandSet>
