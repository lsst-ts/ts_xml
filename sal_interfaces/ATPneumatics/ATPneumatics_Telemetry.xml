<?xml version="1.0" encoding="UTF-8"?>
<?xml-stylesheet type="text/xsl" href="http://lsst-sal.tuc.noao.edu/schema/SALTelemetrySet.xsl"?>
<SALTelemetrySet xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="http://lsst-sal.tuc.noao.edu/schema/SALTelemetrySet.xsd">
  <SALTelemetry>
    <Subsystem>ATPneumatics</Subsystem>
    <Version>3.7.1</Version>
    <Author/>
    <EFDB_Topic>ATPneumatics_m1AirPressure</EFDB_Topic>
    <Description>Measured pressure in air line to M1 pneumatic actuators</Description>
    <item>
      <EFDB_Name>pressure</EFDB_Name>
      <Description>Measured pressure of M1 air line</Description>
      <IDL_Type>double</IDL_Type>
      <Units>Pa</Units>
      <Count>1</Count>
    </item>
  </SALTelemetry>
  <SALTelemetry>
    <Subsystem>ATPneumatics</Subsystem>
    <Version>3.7.1</Version>
    <Author/>
    <EFDB_Topic>ATPneumatics_m2AirPressure</EFDB_Topic>
    <Description>Measured pressure of airline to M2 pneumatic actuator (Hardware not functional, see DM-20577)</Description>
    <item>
      <EFDB_Name>pressure</EFDB_Name>
      <Description>Measured pressure of M2 air line</Description>
      <IDL_Type>double</IDL_Type>
      <Units>Pa</Units>
      <Count>1</Count>
    </item>
  </SALTelemetry>
  <SALTelemetry>
    <Subsystem>ATPneumatics</Subsystem>
    <Version>3.7.1</Version>
    <Author/>
    <EFDB_Topic>ATPneumatics_mainAirSourcePressure</EFDB_Topic>
    <Description>Measured pressure in main supply line from compressor</Description>
    <item>
      <EFDB_Name>pressure</EFDB_Name>
      <Description>Measured pressure of main air supply line</Description>
      <IDL_Type>double</IDL_Type>
      <Units>Pa</Units>
      <Count>1</Count>
    </item>
  </SALTelemetry>
  <SALTelemetry>
    <Subsystem>ATPneumatics</Subsystem>
    <Version>3.7.1</Version>
    <Author/>
    <EFDB_Topic>ATPneumatics_loadCell</EFDB_Topic>
    <Description>Load cell value (from M1 hardpoint)</Description>
    <item>
      <EFDB_Name>cellLoad</EFDB_Name>
      <Description>Load cell measurement</Description>
      <IDL_Type>double</IDL_Type>
      <Units>kg</Units>
      <Count>1</Count>
    </item>
  </SALTelemetry>
</SALTelemetrySet>
